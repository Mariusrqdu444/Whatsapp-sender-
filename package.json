{
  "name": "whatsapp-sender",
  "version": "1.0.0",
  "description": "WhatsApp messaging automation tool",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build",
    "install-all": "npm install && npm run client-install",
    "build-all": "npm run client-build",
    "postinstall": "npm run client-install && npm run client-build"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}